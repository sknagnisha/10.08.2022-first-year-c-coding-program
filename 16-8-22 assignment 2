//c program to printf prime numbers from right to left using arrays
#include<stdio.h>
int main()
{
 int a[5],i,j,flag;
 printf("enter array elements");
 for(i=0;i<5;i++)
 scanf("%d",&a[i]);
 printf("the prime numbers are:");
 for(i=4;i>=0;i--)
 {
  flag=0;
  for(j=2;j<a[i];j++)
  {
   if(a[i]%j==0)
   {
    flag=1;
    break;
   }
   }
   if(flag==0)
   printf("%d\t",a[i]);
   }
   return 0;
   }
   output:
   enter array elements:1
   3
   7
   33
   97
   the prime numbers are:97 7 3 1

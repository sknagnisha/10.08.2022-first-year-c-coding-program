#include <stdio.h>

int main() {
int arr[100][100],i,j,r,c,sum=0;
scanf("%d%d",&r,&c);
for(i=0;i<r;i++)
{
    for(j=0;j<c;j++)
    scanf("%d",&arr[i][j]);
    printf("\n");
}
for(i=0;i<r;i++)
{
    sum=0;
    for(j=0;j<c;j++)
    {
    sum=sum+arr[i][j];
    }
    printf("%d row sum=%d\n",i+1,sum);
    
}
   

    return 0;
}

#include <stdio.h>
#include<string.h>

int main() {
char s1[50],s2[50];
int i,j,temp,temp1,n;
gets(s1);
gets(s2);
n=strlen(s1);
for(i=0;i<n-1;i++)
{
    for(j=0;j<n-i-1;j++)
    {
        if(s1[j]>s1[j+1])
        {
            temp=s1[j];
            s1[j]=s1[j+1];
            s1[j+1]=temp;
        }
    }
}
puts(s1);

for(i=0;i<n-1;i++)
{
    for(j=0;j<n-i-1;j++)
    {
        if(s2[j]>s2[j+1])
        {
            temp1=s2[j];
            s2[j]=s2[j+1];
            s2[j+1]=temp1;
        }
    }
}
puts(s2);
int count=0;
for(i=0;i<n;i++)
{
    if(s1[i]==s2[i])
    count++;
}
if(count==n)
printf("Anagram ");
else
printf("not an anagram");
    return 0;
}
